/* eslint-disable class-methods-use-this */
import BaseModel from '@/model/BaseModel'

// 我们通过 class 这样的语法糖使模型这个概念更加具象化，其优点：耦合性低、可维护性。
class <%= modelName.charAt(0).toUpperCase() + modelName.slice(1) %> extends BaseModel {
  resourceBaseUrl = '<%= endpointBasePath %>'

  // constructor() {}
}

export default new <%= modelName.charAt(0).toUpperCase() + modelName.slice(1) %>()
